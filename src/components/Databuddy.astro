---
export interface Props {
  websiteId: string | undefined;
}

const { websiteId } = Astro.props;
---

{
  websiteId && (
    <script
      is:inline
      defer
      src='https://cdn.databuddy.cc/databuddy.js'
      crossorigin='anonymous'
      async
      data-client-id={websiteId}
    />
  )
}
